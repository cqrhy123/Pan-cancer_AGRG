library(corrplot)                                              #引用包
setwd("E:\\pancancer\\corrplot")        #设置工作目录
rt=read.table("Pancancercorexp.txt",sep="\t",header=T,row.names=1,check.names=F)    #读取输入文件
#绘制相关性图形
pdf("corrplot.pdf",height=10,width=10)              #保存图片的文件名称
par(oma=c(0.5,1,1,1.2))
M=cor(t(rt))
corrplot(M, order = "AOE", type = "upper", tl.pos = "lt")
corrplot(M, add = TRUE, type = "lower", method = "number", order = "AOE",
col = "black", diag = FALSE, tl.pos = "n", cl.pos = "n")
dev.off()
